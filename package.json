{
  "name": "jekyll-postcss-browserify",
  "version": "0.1.0",
  "description": "A boilerplate for building static websites with Gulp, Jekyll, PostCSS, Browserify, svg-sprite, and more.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install",
    "start": "gulp",
    "production": "gulp production",
    "deploy": "gulp deploy",
    "build": "gulp build",
    "build-production": "gulp buildProduction",
    "test-scripts": "standard test/*.js && tape test/*.js | faucet",
    "test-html": "bundle exec htmlproofer ./build/production --only-4xx --check-favicon --check-html --disable-external",
    "test": "npm run test-scripts && npm run test-html"
  },
  "author": "Chris Bracco",
  "license": "MIT",
  "keywords": [
    "jekyll",
    "postcss",
    "browserify",
    "gulp",
    "static site generator",
    "boilerplate",
    "website",
    "blog"
  ],
  "engines": {
    "node": ">= 8.11.3"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "standard": "^6.0.8",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "browser-sync": "^2.26.3",
    "browserify": "^13.3.0",
    "css-mqpacker": "^4.0.1",
    "cssnano": "^3.10.0",
    "del": "^2.2.2",
    "gulp": "^4.0.0",
    "gulp-changed": "^1.3.2",
    "gulp-gzip": "^1.4.2",
    "gulp-html-replace": "^1.6.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.4.0",
    "gulp-rev": "^7.1.2",
    "gulp-rev-collector": "^1.3.1",
    "gulp-rsync": "0.0.5",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.12.1",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.8",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^8.2.0",
    "postcss-remove-root": "0.0.2",
    "postcss-url": "^8.0.0",
    "pretty-hrtime": "^1.0.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0"
  }
}
